<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="local.AppMain">
    <method name="registrationAppID">
      <arg name="appid" type="i" direction="in"/>
      <arg name="appname" type="s" direction="in"/>
      <arg name="servicename" type="s" direction="in"/>
      <arg name="objectpath" type="s" direction="in"/>
    </method>
    <method name="deregistrationAppID">
      <arg name="appid" type="i" direction="in"/>
      <arg name="appname" type="s" direction="in"/>
    </method>
    <method name="requestShow">
      <arg name="appid" type="i" direction="in"/>
    </method>
  </interface>
</node>
